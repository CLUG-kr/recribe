<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pm717.recribe.beans.dao.UserDAO">

	<select id="isUserIdDuplicate" resultType="int">
		SELECT count(*) FROM user WHERE user_id = #{id}
	</select>
	
	<select id="isUserNicknameDuplicate" resultType="int">
		SELECT count(*) FROM user WHERE user_nickname = #{nickname}
	</select>
</mapper>








